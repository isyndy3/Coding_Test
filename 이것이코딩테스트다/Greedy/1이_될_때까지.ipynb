      "source": [
        "#입력 받기\n",
        "n, k = map(int, input().split())\n",
        "\n",
        "cnt = 0\n",
        "while n > 1:\n",
        "  if n % k == 0: #n이 k로 나누어지면\n",
        "    n = n // k\n",
        "    cnt += 1\n",
        "\n",
        "  else: #n이 k로 나누어지지 않으면\n",
        "    n -= 1\n",
        "    cnt += 1\n",
        "\n",
        "print(cnt)"
      ]
    }
  ]
